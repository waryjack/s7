 <div class="s7 flex-item inset">
             <h3 class="inv-header">
                <div>Armor</div>
                        <a class="add-item" title="Add Armor" data-type="armor">
                            <i class="fas fa-plus-square"></i>
                        </a>
            </h3>
            <table class="item-table">
                <tr>
                        <th style="text-align:left;">Armor</th>
                        <th>Rating (Curr/Max)</th>
                        <th>Elemental Protection (A/C/E/F)</th>
                        <th>Equipped</th>
                        <th>Edit</th>
                </tr>
            {{#each armors as |armor name|}}
            <tr style="text-align:center;">
                <td style="text-align:left;"><strong><a class="item-name item" data-item-id="{{armor._id}}">{{armor.name}}</a></strong></td>
                <td>{{armor.data.current_rating}}/{{armor.data.rating}}</td>
                <td>{{armor.data.element.acid}}/{{armor.data.element.cold}}/{{armor.data.element.electricity}}/{{armor.data.element.fire}}</td>
                <td class="item-name item" data-item-id="{{armor._id}}"><input class="equip-item" style="height:14px;" type="checkbox" data-field="data.equipped" {{checked armor.data.equipped}}/></td>
                <td><a class="item-edit item" data-item-id="{{armor._id}}"><i class="fas fa-edit"></i></a> <a class="item-delete item" data-item-id="{{armor._id}}"><i class='fas fa-trash'></i></a></td>
            </tr>
            {{/each}}
            </table>
    </div>